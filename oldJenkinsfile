node {
    def app
     stage ('Clone'){
        checkout scm
     }
     stage ('Build images'){
        app = docker.build("tag1/httpd")
     }
     stage ('Test image'){
        withDockerContainer(args: '--rm',"tag1/httpd"){ 
            sh "echo 'hello world'" 
            }
            sh "docker ps"
            }
}
